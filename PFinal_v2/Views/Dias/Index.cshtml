@model IEnumerable<PFinal_v2.Models.Dia>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UsuarioId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WbsId)
            </th>
            
            <th>
                @Html.DisplayNameFor(model => model.DiaData)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Horas)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UsuarioId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WbsId)
            </td>
           
            <td>
                @Html.DisplayFor(modelItem => item.DiaData)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Horas)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.DiaId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.DiaId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.DiaId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
